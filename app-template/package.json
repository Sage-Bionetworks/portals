{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@types/jest": "24.0.9",
    "@types/node": "11.10.4",
    "@types/pendo-io-browser": "^2.16.1",
    "@types/react": "^16.8.10",
    "@types/react-dom": "16.8.2",
    "clone-deep": "^4.0.1",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.1.5",
    "react": "16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-cookie": "4.0.0",
    "react-dom": "^16.8.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "react-spinners": "^0.5.3",
    "sass": "^1.5.1",
    "source-map-explorer": "^1.6.0",
    "synapse-react-client": "1.12.25",
    "typescript": "3.4.5"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "yarn run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "copy-favicon": "cp src/config/style/favicon* public/",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn copy-favicon; yarn run build-css && CI=false react-scripts build",
    "copy-example-configuration": "cp -r src/example-configuration/ src/config/",
    "test": "yarn run copy-example-configuration && react-scripts test --env=jsdom --setupFiles ./src/tests/setupTests.ts",
    "generate-sitemap": "node sitemap/generate-sitemap.js",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/katex": "^0.5.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "isomorphic-fetch": "^2.2.1",
    "katex": "^0.10.0-rc.1",
    "markdown-it": "^8.4.2",
    "markdown-it-br": "^1.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emphasis-alt": "^1.0.8",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-sub-alt": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "markdown-it-synapse": "^1.1.1",
    "markdown-it-synapse-heading": "^1.0.1",
    "markdown-it-synapse-math": "^3.0.4",
    "markdown-it-synapse-table": "^1.0.6",
    "node": "^12.3.1",
    "sanitize-html": "^1.18.4",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-react": "^3.6.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not ie <= 10",
      "not ie_mob <= 10",
      "not bb <= 10",
      "not op_mob <= 12.1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "js-yaml": "3.13.1"
  }
}
