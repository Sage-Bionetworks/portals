{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.9.1",
    "@sage-bionetworks/react-base-table": "^1.12.3",
    "clone-deep": "^4.0.1",
    "lodash-es": "^4.17.21",
    "plotly.js-basic-dist": "^1.47.3",
    "react": "18.1.0",
    "react-app-polyfill": "^1.0.1",
    "react-bootstrap": "^1.5.2",
    "react-cookie": "4.0.0",
    "react-dom": "18.1.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-spinners": "^0.11.0",
    "react-use": "^17.3.1",
    "sass": "^1.30.0",
    "synapse-react-client": "3.0.11"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(synapse-react-client|lodash-es|jest*)/)"
    ]
  },
  "scripts": {
    "build-css": "sass src/:src/ --load-path=src/config/style --load-path=src/style --load-path=node_modules",
    "watch-css": "yarn run build-css && sass src/ --watch --load-path=src/config/style --load-path=src/style --load-path=node_modules",
    "start-js": "craco start",
    "copy-favicon": "cp src/config/style/favicon* public/",
    "save-build-date": "date > public/build-date.txt",
    "start": "npm-run-all -p watch-css start-js",
    "build": "yarn copy-favicon; yarn save-build-date; yarn run build-css && CI=false GENERATE_SOURCEMAP=false craco --max_old_space_size=7168 build",
    "copy-test-configuration": "cp -r src/test-configuration/ src/config/",
    "test": "yarn run copy-test-configuration && craco test --env=jsdom --setupFiles ./src/tests/setupTests.ts",
    "generate-sitemap": "node sitemap/generate-sitemap.js",
    "eject": "craco eject",
    "analyze": "yarn run copy-test-configuration && craco --max_old_space_size=4096 build && source-map-explorer 'build/static/js/*.js'",
    "link-src": "yarn link synapse-react-client && yarn link react && yarn link react-router && yarn link react-router-dom",
    "unlink-src": "yarn unlink synapse-react-client && yarn unlink react && yarn unlink react-router && yarn unlink react-router-dom && yarn install --check-files"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.5.0",
    "@types/katex": "^0.5.0",
    "@types/node": "11.10.4",
    "@types/plotly.js": "^1.54.10",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-tooltip": "^4.2.4",
    "assert": "^2.0.0",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "katex": "^0.10.0-rc.1",
    "markdown-it": "^8.4.2",
    "markdown-it-br": "^1.0.0",
    "markdown-it-center-text": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-emphasis-alt": "^1.0.8",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-strikethrough-alt": "^1.0.0",
    "markdown-it-sub-alt": "^1.0.0",
    "markdown-it-sup-alt": "^1.0.2",
    "markdown-it-synapse": "^1.1.7",
    "markdown-it-synapse-heading": "^1.0.1",
    "markdown-it-synapse-math": "^3.0.4",
    "markdown-it-synapse-table": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pluralize": "^8.0.0",
    "postcss-normalize": "^10.0.1",
    "process": "^0.11.10",
    "react-scripts": "5.0.1",
    "sanitize-html": "^1.18.4",
    "source-map-explorer": "^2.5.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "timers-browserify": "^2.0.12",
    "typescript": "4.6.4",
    "url": "^0.11.0",
    "util": "^0.12.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "not ie <= 10",
      "not ie_mob <= 10",
      "not bb <= 10",
      "not op_mob <= 12.1"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "babel-jest": "^27.5.1",
    "js-yaml": "3.13.1",
    "plotly.js-basic-dist": "^1.47.3",
    "react": "18.1.0",
    "@types/plotly.js": "^1.54.10",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3"
  }
}
